html 
    head 
        title=title 
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel='stylesheet', href='/views/style.css')
        link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fomantic-ui@2.8.8/dist/semantic.min.css")
    body
      center
        - var cnt=1
        h3 MEN
        P
        table#tb_rank_male(width="430px")
            tr 
                th 
                th
                th 인스타ID
                th 닉네임
                th S
                th B
                th D
                th Total
            ul
                each user in rankData 
                  if user.sex==="male"
                    tr
                        td= cnt++
                        td(style="padding:0px;" width="50px") 
                            img(src="/images/profile/"+ user.instaID +".jpg" width="50px" height="50px")
                        td(width="130px") 
                            a(href="http://www.instagram.com/"+user.instaID)
                                =user.instaID
                        td= user.nickName
                        td= user.squat
                        td= user.benchpress
                        td= user.deadlift
                        td= user.total
        P
        h3 WOMEN
        P
        - cnt=1
        table#tb_rank_female(width="430px")
            tr
                th  
                th
                th 인스타ID
                th 닉네임
                th S
                th B
                th D
                th Total
            ul
                each user in rankData 
                  if user.sex==="female"
                    tr
                        td= cnt++
                        td(style="padding:0px;" width="50px") 
                            img(src="/images/profile/"+ user.instaID +".jpg" width="50px" height="50px")
                        td(width="130px") 
                            a(href="http://www.instagram.com/"+user.instaID)
                                =user.instaID
                        td= user.nickName
                        td= user.squat
                        td= user.benchpress
                        td= user.deadlift
                        td= user.total

        script(src="/script/ranking.js")
        script(src="//code.jquery.com/jquery-1.11.0.min.js")
        script(src="https://cdn.jsdelivr.net/npm/fomantic-ui@2.8.8/dist/semantic.min.js")