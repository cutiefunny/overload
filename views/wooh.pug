html 
    head 
        title=title 
        meta(charset="UTF-8")
        //-meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel='stylesheet', href='/views/style.css')
        //-link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fomantic-ui@2.8.8/dist/semantic.min.css")
    body
        -var tdCnt=0;
        -var trCnt=0;
        center
            P 
            button(onclick="site1()") 우현상품권
            |  
            button(onclick="site2()") 명동상품권
            |  
            button(onclick="site3()") 우천상품권
            P
            table
                tr 
                    th
                    each item in name 
                        th(colspan="2")= item
                tbody
                    each item in data
                        tr
                        - trCnt=0;
                            td= item.time
                            - tdCnt=0;
                            each buys in item.buy
                                td(style="color:red")= buys
                                td(style="color:blue")= item.sell[tdCnt++]
                        -trCnt++;
                //-     tr= time
                    //- each item in data["buy"]
                    //-     td(style="color:red")= item
                        //-td(style="color:blue")= sell[cnt++]
            P 
            P 
            button(onclick="get()") get

        script(src="//code.jquery.com/jquery-1.11.0.min.js")
        //-script(src="https://cdn.jsdelivr.net/npm/fomantic-ui@2.8.8/dist/semantic.min.js")
        //-script(src="/script/dropdown.js")
        script(src="/script/wooh.js")